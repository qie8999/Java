package loopTest1216;

import java.util.Scanner;

public class Loop5 {

	public static void main(String[] args) {
		Scanner s = new Scanner(System.in);
		System.out.println("원하는 구구단 입력: ");
		int gugu = s.nextInt();
		int gop = 0;
		
		for(int i=0; i<9; i++) {
			gop += (i+1);
		}
		System.out.println(gugu + "*" + gop + "=" + (gugu * gop));


	}

}
