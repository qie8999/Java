package loopTest1216;

public class Loop7 {

	public static void main(String[] args) {
		int hap = 0;
		for (int i=0; i<10; i++) {
			if((i+1) % 2 == 0) {
				//continue를 만나는 순간 실행하지 않고 
				//반복문 처음으로 이동
				continue;
			}
			hap += (i+1);
		}
		System.out.println("1~10까지 3의 배수를 제외한 합 : " + hap);

	}

}
